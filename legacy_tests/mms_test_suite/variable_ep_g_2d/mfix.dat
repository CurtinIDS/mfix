!  BC_U_g(3)   = 5.0   
!  BC_V_g(3)   = 5.0     
!  BC_W_g(3)   = 5.0
!  BC_U_s(3,1) = 5.0
!  BC_V_s(3,1) = 5.0
!  BC_W_s(3,1) = 5.0
!  BC_P_g(3)   = 0.0
!  BC_T_g(3)   = 350.0
!  BC_T_s(3,1) = 300.0
!  BC_Theta_m(3,1) =100.0

! East: set as MI for MMS
!---------------------------------------------------------------------//

  BC_X_w(4)   = 1.0
  BC_X_e(4)   = 1.0
  BC_Y_s(4)   = 0.0
  BC_Y_n(4)   = 1.0 
!  BC_Z_b(4)   = 0.0
!  BC_Z_t(4)   = 1.0

  BC_type(4)  = 'MI'

  BC_EP_g(4)  = 0.7   
  BC_U_g(4)   = -5.0   
  BC_V_g(4)   = 5.0     
!  BC_W_g(4)   = 5.0
  BC_U_s(4,1) = -5.0
  BC_V_s(4,1) = 5.0
!  BC_W_s(4,1) = 5.0
  BC_P_g(4)   = 0.0
  BC_T_g(4)   = 350.0
  BC_T_s(4,1) = 300.0
