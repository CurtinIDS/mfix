  BC_Y_s(5)   = 1.0
  BC_Y_n(5)   = 1.0
!  BC_Z_b(5)   = 0.0
!  BC_Z_t(5)   = 1.0

  BC_type(5)  = 'MI'

  BC_EP_g(5)  = 0.7   
  BC_U_g(5)   = 5.0   
  BC_V_g(5)   = -5.0     
!  BC_W_g(5)   = 5.0
  BC_U_s(5,1) = 5.0
  BC_V_s(5,1) = -5.0
!  BC_W_s(5,1) = 5.0
  BC_P_g(5)   = 0.0
