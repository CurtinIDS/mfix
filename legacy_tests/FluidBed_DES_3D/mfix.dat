  BC_X_e(1) =       1.2     ! (m)
  BC_Z_b(1) =       0.0     ! (m)
  BC_Z_t(1) =       1.2     ! (m)
  BC_Y_s(1) =       5.0     ! (m)
  BC_Y_n(1) =       5.0     ! (m)

