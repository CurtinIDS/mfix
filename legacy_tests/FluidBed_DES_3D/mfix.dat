  BC_P_g(1)   =     0.0     ! (barye)
  BC_T_g(1)   =   300.0     ! (K)


! Walls:
!---------------------------------------------------------------------//
  BC_TYPE(3) = 'CG_NSW'



! Distributor: (Gas only)
!---------------------------------------------------------------------//

