  IC_P_g(2) =       0.0     ! (bary)
  IC_T_g(2) =     300.0     ! (K)

  IC_U_g(2) =       0.0     ! (cm/sec)
  IC_W_g(2) =       0.0     ! (cm/sec)
  IC_V_g(2) =       0.0     ! (cm/sec)

  IC_EP_s(2,1) =    0.15    ! (-)

  IC_U_s(2,1) =     0.0     ! (cm/sec)
  IC_V_s(2,1) =     0.0     ! (cm/sec)
  IC_W_s(2,1) =     0.0     ! (cm/sec)

  IC_THETA_M(2,1) = 0.0

#_______________________________________________________________________
# BOUNDARY CONDITIONS SECTION


! Pressure Outlet
!---------------------------------------------------------------------//
