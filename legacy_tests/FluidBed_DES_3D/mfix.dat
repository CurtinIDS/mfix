  RUN_NAME    = 'CYL'
  DESCRIPTION = '3D Cylinder with discrete solids (STL geometry).'

#_______________________________________________________________________
# RUN CONTROL SECTION

  RUN_TYPE = 'NEW'
  UNITS = 'CGS'

  Time   =   0.0
  Tstop  =   0.1

  DT =     @(1.0/128.0)
  DT_MAX = @(1.0/128.0)

  ENERGY_EQ  = .F.
  SPECIES_EQ = .F.   .F.

  CALL_USR = .F.

  GENER_PART_CONFIG = .T.

  DES_INTERP_ON = .T.
  DES_INTERP_MEAN_FIELDS = .T.

  DES_INTERP_SCHEME = 'GARG_2012'

  DRAG_TYPE = 'GIDASPOW'

#_______________________________________________________________________
# NUMERICAL SECTION

! Set the max-number of iterations before "stalled"
  MAX_NIT = 50


#_______________________________________________________________________
# GEOMETRY SECTION

  COORDINATES = 'CARTESIAN'

  XLENGTH =   1.2   IMAX = 12
  ZLENGTH =   1.2   KMAX = 12
  YLENGTH =   5.0   JMAX = 25

  CARTESIAN_GRID = .T.

  USE_STL = .T.
  STL_BC_ID = 3

  TX_STL = -2.0
  TY_STL =  0.0
  TZ_STL = -2.0

  FAC_DIM_MAX_CUT_CELL = 1.0

  PRINT_PROGRESS_BAR = .T.

#_______________________________________________________________________
# MATERIAL SECTION

! Gas Section
!---------------------------------------------------------------------//
  MU_g0 = 1.8E-4    ! constant gas viscosity
  RO_g0 = 1.2E-3    ! constant gas density


! Particle Section
!---------------------------------------------------------------------//

  MMAX = 1

  PARTICLES = 5000   ! Number of particles

  KN = 1.0d+6        ! Normal inter-particle collision spring constant
  KT_FAC = @(2/7)    ! Tangential spring factor = KT/KN
  MEW = 0.0          ! Inter-particle friction coeffienct

  KN_W = 1.0d+6      ! Normal particle-wall collision spring constant
  KT_W_FAC = @(2/7)  ! Tangential spring factor = KT_W/KN_W
  MEW_W = 0.0        ! Particle-wall friction coefficient

  DES_EN_INPUT =      1.d0
  DES_EN_WALL_INPUT = 1.d0

  NFACTOR = 0        ! No initial settling

! Solids phase 1:
!---------------------------------------------------------------------//
  SOLIDS_MODEL(1) = 'DEM'

  RO_s0(1) =   2.00   ! solids density
  D_p0(1) =    0.04   ! particle diameter


#_______________________________________________________________________
# INITIAL CONDITIONS SECTION

! Freeboard
!---------------------------------------------------------------------//
  IC_X_w(1) =       0.0     ! (cm)
  IC_X_e(1) =       1.2     ! (cm)
  IC_Z_b(1) =       0.0     ! (cm)
  IC_Z_t(1) =       1.2     ! (cm)
  IC_Y_n(1) =       5.0     ! (cm)
  IC_Y_s(1) =       0.0     ! (cm)

  IC_EP_g(1) =      1.0     ! (-)

  IC_P_g(1) =       0.0     ! (bary)
  IC_T_g(1) =     300.0     ! (K)

  IC_U_g(1) =       0.0     ! (cm/sec)
  IC_W_g(1) =       0.0     ! (cm/sec)
  IC_V_g(1) =       0.0     ! (cm/sec)


! Bed
!---------------------------------------------------------------------//
  IC_X_w(2) =       0.0     ! (cm)
  IC_X_e(2) =       1.2     ! (cm)
  IC_Z_b(2) =       0.0     ! (cm)
  IC_Z_t(2) =       1.2     ! (cm)
  IC_Y_n(2) =       2.0     ! (cm)
  IC_Y_s(2) =       0.0     ! (cm)

