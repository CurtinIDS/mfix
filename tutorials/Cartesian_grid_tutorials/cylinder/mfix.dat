  NCY         = 20      40     20
                        
  LAST_DY(1)  = -1.0  ! Match Last  DY from segment 1 with First DY of segment 2
  FIRST_DY(3) = -1.0  ! Match First DY from segment 3 with Last  DY of segment 2
 

